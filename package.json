{
  "name": "lsa",
  "version": "1.0.0",
  "productName": "lsa",
  "main": "./src/main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "pure-electron": "electron .",
    "pure-electron-dev": "NODE_ENV=development electron .",
    "electron": "run-s build pure-electron",
    "electron-dev": "run-p dev pure-electron-dev",
    "rebuild-32": "electron-rebuild -f -w sqlanywhere --arch=ia32",
    "rebuild-64": "electron-rebuild -f -w sqlanywhere --arch=x64",
    "package-32": "electron-packager . --platform=win32 --arch=ia32 --asar --out=out --overwrite",
    "package-64": "electron-packager . --platform=win32 --arch=x64 --asar --out=out --overwrite",
    "build-package-32": "run-s build rebuild-32 package-32",
    "build-package-64": "run-s build rebuild-64 package-64",
    "make-installer": "node tasks/electron-winstaller.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "chokidar": "^3.3.1",
    "electron": "^4.2.12",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "electron-wix-msi": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "sqlanywhere": "^1.0.24"
  }
}
